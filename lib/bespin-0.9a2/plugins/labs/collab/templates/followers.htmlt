<div>${title}</div>
<table>
    <tbody>
        <tr foreach="${followers}">
            <td><img width="16" height="16" class="social_user_name_${param.name}" src="${param.url}"></td>
            <td>${param.name}</td>
            <td><a href="#unfollow" data-arg0="${param.name}" onclick="${onUnfollow}">(unfollow)</a></td>
        </tr>
    </tbody>
</table>
